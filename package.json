{
  "author": "danthonywalker",
  "dependencies": {
    "@googleapis/youtube": "11.0.1",
    "canvas": "2.11.2",
    "compress-tag": "3.0.0",
    "cron": "2.4.4",
    "csv-stringify": "6.4.2",
    "discord.js": "14.13.0",
    "express": "4.18.2",
    "express-prom-bundle": "6.6.0",
    "glob": "10.3.7",
    "ioredis": "5.3.2",
    "pg": "8.11.3",
    "pino": "8.15.1",
    "pino-http": "8.5.0",
    "prom-client": "14.2.0",
    "rss-parser": "3.13.0",
    "vega": "5.25.0"
  },
  "devDependencies": {
    "@types/express": "4.17.18",
    "@types/pg": "8.10.3",
    "@typescript-eslint/eslint-plugin": "6.7.3",
    "@typescript-eslint/parser": "6.7.3",
    "eslint": "8.50.0",
    "eslint-plugin-unicorn": "48.0.1",
    "pino-pretty": "10.2.0",
    "prettier": "3.0.3",
    "shx": "0.3.4",
    "typescript": "5.2.2"
  },
  "license": "MIT",
  "private": true,
  "name": "pedestrian",
  "scripts": {
    "build": "shx rm -rf ./build && tsc",
    "check": "tsc --noemit && eslint . && prettier --check .",
    "dump": "docker exec --tty pedestrian-postgres-1 pg_dumpall --clean --username=user > dump.sql",
    "migrate": "docker rm pedestrian-flyway-1 && docker rmi pedestrian-flyway && npm run services",
    "prettier": "prettier --write .",
    "restore": "shx cat dump.sql | docker exec --interactive pedestrian-postgres-1 psql --username=user --dbname=postgres",
    "services": "docker-compose up --detach",
    "start": "node --trace-warnings --enable-source-maps build/index.js | pino-pretty"
  }
}
